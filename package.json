{
  "name": "gemini-live-backend",
  "version": "1.0.0",
  "description": "Backend for Gemini Live Audio Chat",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "nodemon --exec 'node --loader ts-node/esm' src/server.ts",
    "dev:frontend": "vite",
    "dev:backend": "nodemon --exec 'node --loader ts-node/esm' src/server.ts",
    "dev:all": "concurrently \"pnpm dev:backend\" \"pnpm dev:frontend\""
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@google/genai": "^0.14.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "@types/ws": "^8.5.10",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.5.2",
    "vite": "^5.0.0",
    "concurrently": "^8.2.2"
  }
}